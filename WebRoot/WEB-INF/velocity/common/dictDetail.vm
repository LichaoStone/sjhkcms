<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<title>字典信息</title>
<meta http-equiv="content-type" content="text/html; charset=UTF-8">
#tagLibs()
</head>
<body>
	<table width="98%"  border="0" align="center" cellpadding="2" cellspacing="2">
        <tr>
         <td colspan="2">
				<legend>字典信息</legend>
					<table width="98%" class="pn-ftable" cellpadding="2" cellspacing="1" border="0">
						<tr>
							<td width="20%" class="pn-flabel pn-flabel-h">所属字典：</td>
							<td class="pn-fcontent">
								$!{dict.dicType}
							</td>
							<td width="20%" class="pn-flabel pn-flabel-h">字典名称：</td>
							<td class="pn-fcontent">
								$!{dict.dicName}
							</td>
						</tr>
						<tr>
							<td width="20%" class="pn-flabel pn-flabel-h">字典值：</td>
							<td class="pn-fcontent">
								$!{dict.dicValue}
							</td>
							<td width="20%" class="pn-flabel pn-flabel-h">顺序：</td>
							<td class="pn-fcontent">
								$!{dict.orderId}
							</td>
						</tr>
						<tr>
							<td width="20%" class="pn-flabel pn-flabel-h">创建人：</td>
							<td class="pn-fcontent">
								$!{dict.creator}
							</td>
							<td width="20%" class="pn-flabel pn-flabel-h">字典状态：</td>
							<td class="pn-fcontent">
								$!{dict.statusName}
							</td>
						</tr>
						<tr>
							<td width="20%" class="pn-flabel pn-flabel-h">备注：</td>
							<td class="pn-fcontent" colspan="3">
									$!{dict.memo}
							</td>
						</tr>
					</table>
		</td>
    </tr>
   <tr>
    <td style="padding-top:20px;">
		<button type="button" onClick="history.go(-1)" class="bt-secondary">返回</button>
    </td>
   </tr>
</table>
</body>
</html>
